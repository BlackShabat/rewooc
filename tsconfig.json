{
  "include": ["src/**/*"],
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    // TODO remove after renaming all to .ts
    "noImplicitAny": false,
    "baseUrl": "./",
    "paths": {
      "app-data": ["src/data"],
      "app-types": ["src/types"]
    }
  }
}
